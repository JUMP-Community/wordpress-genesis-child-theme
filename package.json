	"devDependencies": {
		"@10up/stylelint-config": "^1.1.5",
		"@popperjs/core": "^2.11.2",
		"10up-toolkit": "^3.1.2",
		"bootstrap": "^5.1.3",
		"stylelint": "^14.7.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"stylelint-scss": "^4.2.0",
		"stylelint-webpack-plugin": "^3.2.0"
	},
	"scripts": {
		"start": "10up-toolkit build --watch",
		"build": "10up-toolkit build",
		"format-js": "10up-toolkit format-js",
		"format-style": "stylelint assets/css/* --fix",
		"lint-js": "10up-toolkit lint-js",
		"lint-style": "10up-toolkit lint-style",
		"test": "10up-toolkit test-unit-jest",
		"clean": "rm -rf package-lock.json && rm -rf node_modules && npm install"
	},
	"10up-toolkit": {
		"devURL": "https://jump.local",
		"entry": {
			"shared": "./assets/js/shared/shared.js",
			"admin": "./assets/js/admin/admin.js",
			"frontend": "./assets/js/frontend/frontend.js",
			"shared-style": "./assets/css/shared.scss",
			"admin-style": "./assets/css/admin.scss",
			"editor-style": "./assets/css/editor.scss",
			"frontend-style": "./assets/css/style.scss"
